module staging:

imports:
  module list
  module stack
  module control

$: ap11 swap drop

__ a [f] -> a fa
peek: [dup] dip12 $
->: peek swap

__ a [f] [g] -> fa ga
fork: [peek swap] dip22 $

__ a b [f] [g] -> fa gb
para: [dip11] dip32 $

__ a [cond] [f] -> fa
__ f fails if cond does not return True
if: [peek] dip22 swap [$] dip21 !

__ [fn] [test] initial
iterate: dup2 swap $

check: [|] control.dip21 [dup] swap . . popr !
